{
  "scripts": {
    "start": "npm-run-all --parallel css eleventy browsersync",
    "eleventy": "eleventy --watch",
    "debug": "set DEBUG=* & eleventy",
    "css": "postcss src/static/css/tailwind.css --o static/css/style.css --watch",
    "build": "cross-env ELEVENTY_ENV=dev NODE_ENV=prod eleventy && cross-env NODE_ENV=prod tailwindcss -i src/static/css/tailwind.css -o docs/static/css/style.css",
    "build-gh": "cross-env ELEVENTY_ENV=prod NODE_ENV=prod eleventy && cross-env NODE_ENV=prod tailwindcss -i src/static/css/tailwind.css -o docs/static/css/style.css",
    "browsersync": "browser-sync start --server docs --files docs --port 8080 --no-notify --no-open"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.3",
    "@optimizely/optimizely-sdk": "^5.0.0-beta2",
    "@tailwindcss/typography": "^0.5.0",
    "alpinejs": "^3.7.1",
    "browser-sync": "^2.27.7",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.15",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "luxon": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.1.0",
    "prismjs": "^1.26.0",
    "tailwindcss": "^3.0.13"
  },
  "dependencies": {
    "@11ty/eleventy-upgrade-help": "^2.0.5",
    "autoprefixer": "^10.4.2",
    "dotenv": "^16.0.3",
    "flowbite": "^1.6.4",
    "postcss": "^8.4.5"
  }
}
